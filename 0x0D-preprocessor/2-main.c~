#include <stdio.h>


/**
 * print_filename - prints the name of the current file using print_filename()
 * Return - returns zero
 */



void print_filename(void);

int main(void)
{
print_filename();
return (0);
}

/**
 * main - prints file name
 * Return - returns zero
 */

void print_filename(void)
{
printf("%s\n", __FILE__);
}
